<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.cos.Agora">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_BACKGROUND_LOCATION" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/agora_logo2"
        android:label="Agora"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Daangnapp"
        android:name = "com.cos.Agora.global.User"
        android:usesCleartextTraffic="true">

        <activity android:name="com.cos.Agora.login.AuthActivity" />
        <activity android:name="com.cos.Agora.login.NicknameActivity" />
        <activity android:name="com.cos.Agora.login.LoginActivity" />
        <activity android:name="com.cos.Agora.location.LocationActivity" />
        <activity android:name="com.cos.Agora.splash.StartActivity" />
        <activity android:name="com.cos.Agora.main.MainActivity" />
        <activity android:name="com.cos.Agora.login.JoinActivity" />
        <activity android:name="com.cos.Agora.study.StudyListActivity" />
        <activity android:name="com.cos.Agora.study.StudyCreateActivity" />

        <activity android:name="com.cos.Agora.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>